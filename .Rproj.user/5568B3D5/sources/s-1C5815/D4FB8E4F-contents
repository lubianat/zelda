# This code is meant to plot bipartite networks
# Tiago Lubiana - 18/09/2018

library(GGally)
library(network)
library(igraph)

g  <- graph_from_edgelist(as.matrix(neuro_immune_links[,c("Ligand.ApprovedSymbol", "Receptor.ApprovedSymbol")]))

g_matrix <- as.matrix(igraph::get.adjacency(g))
as.matrix.network(mm.net)


ggbipart::bip_railway(g_matrix, label = T)
save(edgelist, file = '~/Documents/Projects/side/bipartplot/bipart_ex.RData')
